{
  "name": "@fortawesome/angular-fontawesome",
  "version": "4.0.0",
  "description": "Angular Fontawesome, an Angular library",
  "repository": {
    "type": "git",
    "url": "https://github.com/FortAwesome/angular-fontawesome"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FortAwesome/angular-fontawesome/issues"
  },
  "homepage": "https://github.com/FortAwesome/angular-fontawesome",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "tslib": "^2.8.1"
  },
  "keywords": [
    "angular",
    "font",
    "awesome",
    "fontawesome",
    "icon",
    "svg"
  ],
  "peerDependencies": {
    "@angular/core": "^21.0.0"
  },
  "schematics": "./schematics/collection.json",
  "packageManager": "yarn@4.9.2",
  "module": "fesm2022/angular-fontawesome.mjs",
  "typings": "types/angular-fontawesome.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./types/angular-fontawesome.d.ts",
      "default": "./fesm2022/angular-fontawesome.mjs"
    },
    "./testing": {
      "types": "./types/fortawesome-angular-fontawesome-testing.d.ts",
      "default": "./fesm2022/fortawesome-angular-fontawesome-testing.mjs"
    }
  },
  "sideEffects": false
}